{
  "logging_level": "DEBUG",

  "llm": {
    "provider": "turix",
    "model_name": "turix-model",
    "api_key": "your_api_key_here",
    "base_url": "https://llm.turixapi.io/v1"
  },

  "agent": {
    "task": "open Chrome, go to youtube, and search for AI tutorials",
    "short_memory_len": 5,
    "use_ui": false,
    "max_actions_per_step": 5,
    "max_steps": 100,
    "use_turix": true,
    "save_conversation_path": "llm_interactions.log",
    "save_conversation_path_encoding": "utf-8"
  }
}
